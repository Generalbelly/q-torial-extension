{
  "name": "q-torial-extension",
  "version": "0.0.1",
  "description": "q-torial-extension",
  "author": "Nobuyoshi Shimmen <nobuyoshi.shimmen@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "yarn prettier -- --write",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "cross-env  NODE_ENV=production yarn build --watch",
    "watch:dev": "cross-env NODE_ENV=development HMR=true yarn build:dev --watch",
    "tag": "cross-env NODE_ENV=production webpack --hide-modules --config ./webpack.tag.config.js && yarn upload-file",
    "tag:dev": "cross-env NODE_ENV=development webpack --hide-modules --config ./webpack.tag.config.js && yarn upload-file:dev",
    "upload-file": "NODE_ENV=production node scripts/upload.js",
    "upload-file:dev": "NODE_ENV=development node scripts/upload.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@medv/finder": "^1.1.1",
    "axios": "^0.19.0",
    "buefy": "^0.7.5",
    "debounce": "^1.2.0",
    "dompurify": "^1.0.10",
    "firebase": "^6.3.1",
    "pell": "^1.0.6",
    "uuid": "^3.3.2",
    "vee-validate": "^2.2.5",
    "vue": "^2.6.10",
    "vue-i18n": "^8.12.0",
    "vuex": "^3.0.1",
    "webextension-polyfill": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs3": "^7.4.0",
    "@google-cloud/storage": "^4.1.3",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "dotenv-webpack": "^1.7.0",
    "ejs": "^2.6.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^1.1.11",
    "husky": "^2.4.1",
    "j": "^13.0.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.12.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^1.0.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.6.10",
    "web-ext-types": "^2.1.0",
    "webpack": "^4.20.2",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-cli": "^3.1.2"
  }
}
